;---------------------------------------------------------------------------------------------------------
; 说明： 脚本中的字符串区分大小写，用户可以修改"="后面的数值，但是不要修改前面的字符串
;---------------------------------------------------------------------------------------------------------


;--------------------------------------------------------------------------------------------------
;                                   固件下载参数配置
;--------------------------------------------------------------------------------------------------
;****************************************************
;    mbr的大小以及全局对齐大小, 以扇区为单位
;****************************************************
[gpt]
start = 256
align = 256

;********************************************************************************************************
;                                              分区配置
;
;
;  partition 定义范例:
;    [partition]                ;  //表示是一个分区
;    name        = USERFS2      ; //分区名称
;    size        = 16384        ; //分区大小 单位: 扇区.分区表示个数最多2^31 * 512 = 2T
;    downloadfile = "123.fex"   ; //下载文件的路径和名称，可以使用相对路径，相对是指相对于image.cfg文件所在分区。也可以使用绝对路径
;    keydata     = 1            ; //私有数据分区，重新量产数据将不丢失
;    encrypt     = 1            ; //采用加密方式烧录，将提供数据加密，但损失烧录速度
;    ro          = ?            ; //私有用法
;    verify      = 1            ; //要求量产完成后校验是否正确
;    user_type   = 1            ; //用户类型
;    type        =              ; //分区类型GUID
;    uniqueguid  =              ; //分区unique GUID
;    system      =              ; //必需分区属性标志
;    attrhi      =              ; //分区属性描述符高32位
;    attrlo      =              ; //分区属性描述符低32位
;    align       =              ; //指定对应分区对齐大小，如没有指定，采用全局align
;
; 注：1、name唯一, 不允许同名
;     2、name最大12个字符
;     3、size = 0, 将创建一个无大小的空分区
;     4、为了安全和效率考虑，分区大小最好保证为16M字节的整数倍
;********************************************************************************************************
[partition_start]

[partition]
    name         = boot0
    size         = 256
    user_type    = 0x8000
    type         = "E70A8437-2EE2-49C7-8222-6D53BA3E27A2"
    uniqueguid   = "B362D5F2-0960-428E-B865-D53A603F4F4D"
    attrhi       = 0x80000000
    attrlo       = 0x0

[partition]
    name         = uefi
    size         = 4096
    user_type    = 0x8000
    type         = "D2B8B71E-FBC5-4801-98FF-63DC4DF5F3E6"
    uniqueguid   = "34009F11-A04A-440D-BC94-DB019B3F5640"
    attrhi       = 0x80000000
    attrlo       = 0x0

[partition]
    name         = bootloader
    size 	 = 65536
    downloadfile = "boot-resource.fex"
    user_type    = 0x8100
    type         = "EBD0A0A2-B9E5-4433-87C0-68B6B72699C7"
    uniqueguid   = "AA606E73-C0AB-41EE-8F34-F040459CFE74"
    verify       = 1
    attrhi       = 0x80000000
    attrlo       = 0x0

[partition]
    name         = EFIESP
    size         = 65536
    user_type    = 0x8000
    type         = "EBD0A0A2-B9E5-4433-87C0-68B6B72699C7"
    uniqueguid   = "8183040A-8B44-4592-92F7-C6D9EE0560F7"
    attrhi       = 0x80000000
    attrlo       = 0x0

[partition]
